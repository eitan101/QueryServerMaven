    compile 'commons-jxpath:commons-jxpath:1.3'    
    compile 'org.glassfish.jersey.media:jersey-media-json-jackson:2.7'
    compile 'org.glassfish.jersey.containers:jersey-container-jetty-servlet:2.7'-->
  
</project>
